--[[
How to use?
ex code:
local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/kiara-dev-oficial/dead-rails/main/Library"))()
local window = library:CreateWindow("Hi!")

window:CreateButton("Print!", function()
  print("1234")
end)
enabled = false
window:CreateToggle("true or false?", function(idk)
  if idk then
    enabled = true
      spawn(function()
        print(enabled)
      end)
    else
      enabled = false
      print(enabled)
    end
end)
]]--
